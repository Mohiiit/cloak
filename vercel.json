{
  "buildCommand": "cd packages/nextjs && npm run build",
  "outputDirectory": "packages/nextjs/.next",
  "installCommand": "npm install --prefix packages/nextjs --legacy-peer-deps --workspaces=false",
  "crons": [
    {
      "path": "/api/v1/internal/push/dispatch",
      "schedule": "*/1 * * * *"
    }
  ]
}
