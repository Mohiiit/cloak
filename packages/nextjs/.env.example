# URL Devnet
NEXT_PUBLIC_DEVNET_PROVIDER_URL=http://127.0.0.1:5050

# URL Sepolia
NEXT_PUBLIC_SEPOLIA_PROVIDER_URL=https://starknet-sepolia.g.alchemy.com/starknet/version/rpc/v0_9/_hKu4IgnPgrF8O82GLuYU

# URL Mainnet
NEXT_PUBLIC_MAINNET_PROVIDER_URL=https://starknet-mainnet.g.alchemy.com/starknet/version/rpc/v0_9/_hKu4IgnPgrF8O82GLuYU

NEXT_PUBLIC_DEVNET_WS_PROVIDER_URL=ws://127.0.0.1:5050/ws

NEXT_PUBLIC_SEPOLIA_WS_PROVIDER_URL=wss://starknet-sepolia.g.alchemy.com/starknet/version/rpc/v0_9/_hKu4IgnPgrF8O82GLuYU

NEXT_PUBLIC_MAINNET_WS_PROVIDER_URL=wss://starknet-mainnet.g.alchemy.com/starknet/version/rpc/v0_9/_hKu4IgnPgrF8O82GLuYU

# ─── Supabase (server-only, NEVER exposed to client) ───
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJ...your-service-role-key

# MiniMax API key for LLM-powered agent parser (optional — falls back to regex if unset)
MINIMAX_API_KEY=

# Voice transcription (for /api/agent/voice endpoint)
SARVAM_API_KEY=
OPENAI_API_KEY=
VOICE_DEFAULT_PROVIDER=sarvam

# Push-first ward approvals dispatcher
PUSH_DISPATCH_ENABLED=true
PUSH_DISPATCH_SHADOW_MODE=false
PUSH_DISPATCH_SECRET=change-me
PUSH_DISPATCH_BATCH_SIZE=25
PUSH_DISPATCH_LEASE_SECONDS=45
PUSH_DISPATCH_MAX_ATTEMPTS=6
PUSH_DISPATCH_BASE_RETRY_MS=5000
PUSH_DISPATCH_MAX_RETRY_MS=300000

# Web Push (web/extension)
PUSH_VAPID_PUBLIC_KEY=
PUSH_VAPID_PRIVATE_KEY=
PUSH_VAPID_SUBJECT=mailto:ops@cloak.local

# Token push provider for ios/android: none | expo | generic
PUSH_TOKEN_PROVIDER=none

# Expo (when PUSH_TOKEN_PROVIDER=expo)
PUSH_EXPO_ACCESS_TOKEN=

# Generic provider (when PUSH_TOKEN_PROVIDER=generic)
PUSH_TOKEN_PROVIDER_URL=
PUSH_TOKEN_PROVIDER_AUTH_HEADER=
